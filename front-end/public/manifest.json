{
  "name": "Grammy Wammy",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Grammy Wammy",
  "icons": {
      "48": "48X48.png",
      "128": "128X128.png"
  },
  "browser_action": {
      "default_icon": "128X128.png",
      "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["inject-helper/js/jquery-3.1.0.min.js", "background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "clipboardRead",
    "https://www.instagram.com/*",
    "notifications"
    
],

"content_scripts": [
    {
        "all_frames": true,
        "js": [
            "vendor.js",
            "profileGetter.js"
        ],
        "matches": [
            "https://www.instagram.com/"
        ],
        "run_at": "document_end"
    },
    {
        "all_frames": true,
        "js": [
            "vendor.js",
            "messageListGetter.js"
        ],
        "matches": [
            "https://www.instagram.com/direct/inbox/"
        ],
        "run_at": "document_end"
    },
    {
        "all_frames": true,
        "js": [
            "vendor.js",
            "messageIndividual.js"
        ],
        "matches": [
            "https://www.instagram.com/direct/inbox/?id=*",
            "https://www.instagram.com/direct/t/*"
        ],
        "run_at": "document_end"
    }
]
}